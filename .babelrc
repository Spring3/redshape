{
  "presets": [
    [
      "@babel/react", {
        "exclude": ["transform-regenerator", "transform-async-to-generator"],
        "debug": true,
        "modules": false,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "async-to-promises",
    "@babel/plugin-proposal-class-properties",
    ["babel-plugin-styled-components",
      {
        "displayName": true, 
        "preprocess": false 
      }
    ]
  ]
}
