{
  "name": "Redshape",
  "version": "1.1.0",
  "description": "Redmine time tracker",
  "main": "main/index.js",
  "scripts": {
    "start": "yarn prepack && cross-env ELECTRON_IS_DEV=0 electron --noDevServer ./main/index.js",
    "dev": "concurrently 'webpack-dev-server' 'overmind-devtools'",
    "pack": "electron-builder --dir dist",
    "prepack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "yarn prepack && electron-builder",
    "release": "yarn prepack && electron-builder -p always",
    "release:ci": "yarn prepack && electron-builder -p onTagOrDraft",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .js,jsx,.ts,.tsx",
    "test": "tsdx test --watch --max-workers=2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spring3/redshape.git"
  },
  "author": {
    "name": "Spring3 <https://github.com/Spring3>",
    "email": "connect.danv@gmail.com"
  },
  "contributors": [
    {
      "name": "rNoz (Group4Layers member)",
      "email": "rnoz.commits@gmail.com",
      "url": "https://www.group4layers.com"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Spring3/redshape/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "app.spring3.redshape",
    "electronUpdaterCompatibility": ">= 2.16",
    "copyright": "Copyright 2021 danv connect.danv@gmail.com https://www.dvasylenko.com",
    "files": [
      "common/**/*",
      "main/**/*",
      "assets/**/*.@(png|icns|ico)",
      "dist/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Spring3",
        "repo": "redshape"
      }
    ],
    "win": {
      "target": [
        "nsis",
        "nsis-web"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "deb": {
      "category": "Development",
      "depends": [
        "libxss-dev",
        "pkg-config"
      ]
    },
    "appImage": {
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "build/"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@emotion/eslint-plugin": "^11.5.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/hapi__joi": "^17.1.7",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.3.2",
    "@types/showdown": "^1.9.4",
    "@types/styled-components": "^5.1.14",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^4.31.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^27.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "electron": "8.0.1",
    "electron-builder": "22.3",
    "electron-react-devtools": "^0.5.3",
    "electron-reloader": "^1.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "fast-async": "^6.3.8",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.2.3",
    "jest": "^27.2.0",
    "jest-styled-components": "^7.0.5",
    "mini-css-extract-plugin": "^2.3.0",
    "overmind-devtools": "^29.0.1",
    "raw-loader": "^4.0.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "tsdx": "^0.14.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@hapi/joi": "^17.1.0",
    "axios": "^0.19.2",
    "clean-stack": "^2.2.0",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.0.6",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.2",
    "electron-util": "^0.14.0",
    "emotion": "^11.0.0",
    "emotion-theming": "^11.0.0",
    "ensure-error": "^2.0.0",
    "formik": "^2.1.4",
    "got": "^11.8.2",
    "lodash": "^4.17.15",
    "mdi-react": "^6.7.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "overmind": "^28.0.1",
    "overmind-react": "^29.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-cool-inview": "^2.0.8",
    "react-day-picker": "^7.4.0",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.6.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^6.0.2",
    "react-select": "^2.4.1",
    "react-tabs": "^3.1.0",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "showdown": "^1.9.1",
    "styled-components": "^5.0.1",
    "xss": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
